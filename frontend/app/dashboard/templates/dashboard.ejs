<div class="dashboard-rows">
  <% _.forEach(['1', '2', '3', '4'], function(n) { %>
  <div class="panel panel-primary">
    <div class="panel-heading">Care Sensor #3 <%- n %></div>
    <div class="panel-body">
      <dl>
        <dt>Temperatura</dt>
        <dd><span data-tag="cs3.<%- n %>.T">?</span>°C</dd>
        <dt>Wilgotność</dt>
        <dd><span data-tag="cs3.<%- n %>.H">?</span>%</dd>
        <dt>Ruch</dt>
        <dd><span data-tag="cs3.<%- n %>.motion">?</span></dd>
      </dl>
    </div>
  </div>
  <% }) %>
  <% _.forEach(['1', '2'], function(n) { %>
  <div class="panel panel-danger">
    <div class="panel-heading">Care Sensor HT <%- n %></div>
    <div class="panel-body">
      <dl>
        <dt>Temperatura</dt>
        <dd><span data-tag="csht.<%- n %>.T">?</span>°C</dd>
        <dt>Wilgotność</dt>
        <dd><span data-tag="csht.<%- n %>.H">?</span>%</dd>
      </dl>
    </div>
  </div>
  <% }) %>
  <% _.forEach(['1', '2', '3', '4', '5', '6', '7', '8'], function(n) { %>
  <div class="panel panel-warning">
    <div class="panel-heading">Energy Meter <%- n %></div>
    <div class="panel-body">
      <dl>
        <dt>Stała</dt>
        <dd><span data-tag="em.<%- n %>.constant">?</span></dd>
        <dt>Suma</dt>
        <dd>
          <span data-tag="em.<%- n %>.sum">?</span>
          <span data-tag="em.<%- n %>.unit" data-select-value>
            <span class="hidden" data-value="0">kW</span>
            <span class="hidden" data-value="1">m³</span>
            <span class="hidden" data-value="2">cnt</span>
          </span>
        </dd>
        <!--
        <dt>Średnia z minuty</dt>
        <dd>
          <span data-tag="em.<%- n %>.avg">?</span>
          <span data-tag="em.<%- n %>.unit" data-select-value>
            <span class="hidden" data-value="0">kWh</span>
            <span class="hidden" data-value="1">m³</span>
            <span class="hidden" data-value="2">cnt</span>
          </span>
        </dd>
        //-->
      </dl>
    </div>
  </div>
  <% }) %>
</div>
